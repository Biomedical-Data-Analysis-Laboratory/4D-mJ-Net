{
  "EXPERIMENT": 24.4,
  "USE_SEQUENCE_TRAIN": 1,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/SUS2020_TIFF/",
  "PATIENTS_TO_TRAINVAL": ["ALL"],
  "PATIENTS_TO_TEST": ["01_001","01_007","01_013","01_019","01_025","01_031",
    "01_037","01_044","01_049","01_053","01_061","01_067","01_074",
    "02_001","02_007","02_013","02_019","02_025","02_031","02_036",
    "02_043","02_050","02_055","02_062","03_003","03_010","03_014",
    "01_057","01_059","01_066","01_068","01_071","01_073"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 0.4,
    "allow_growth": 1,
    "MULTIPROCESSING": 1
  },
  "relative_paths": {
    "labeled_images": "/home/prosjekt/PerfusionCT/StrokeSUS/GT_TIFF/",
    "patients": "PATIENTS/SUS2020_TIFF/FINAL_TIFF/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "mJNet",
      "loss": "tanimoto_with_dual_loss",
      "metrics": ["squared_dice_coef","dice_coef","tversky_coef","tanimoto","focal_loss","mean_squared_error"],
      "epochs": 10,
      "batch_size": 2,
      "steps_per_epoch_ratio": 1,
      "validation_steps_ratio": 1,
      "classes_to_evaluate": ["penumbra","core","penumbracore"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 15,
        "number_patients_for_validation": 15,
        "number_patients_for_testing": 0,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "SGD",
        "learning_rate": 0.01,
        "decay": 1e-6,
        "momentum":0.9,
        "nesterov":"True"
      },
      "params":{
        "max_pool":{
          "long.1":5,
          "long.2":3,
          "long.3":2
        },
        "dropout":{
          "long.1":0.3,
          "1":0,
          "2":0.3,
          "3":0.3,
          "4":0,
          "5":0
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "val_loss",
          "mode": "min",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "val_loss",
          "min_delta": 0.00001,
          "patience": 25
        },
        "LearningRateScheduler": {
          "decay_step": 25,
          "decay_rate": 0.01
        },
        "CSVLogger": {
          "filename": "logger.csv",
          "separator": ","
        },
        "CollectBatchStats": {
          "acc":"tanimoto"
        }
      },
      "to_categ": 1,
      "save_images": 1,
      "save_statistics": 0,
      "use_background_in_statistics": 0,
      "calculate_ROC": 0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 1,
      "save_activation_filter": 0,
      "use_hickle": 0
    }
  ]
}
