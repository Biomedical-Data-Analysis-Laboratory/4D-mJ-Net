{
  "EXPERIMENT": 20,
  "USE_SEQUENCE_TRAIN": 1,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/SUS2020_TIFF/",
  "PATIENTS_TO_TRAINVAL": ["ALL"],
  "PATIENTS_TO_TEST": ["CTP_01_001","CTP_01_007","CTP_01_013","CTP_01_019","CTP_01_025","CTP_01_031",
    "CTP_01_037","CTP_01_044","CTP_01_049","CTP_01_053","CTP_01_061","CTP_01_067","CTP_01_074",
    "CTP_02_001","CTP_02_007","CTP_02_013","CTP_02_019","CTP_02_025","CTP_02_031","CTP_02_036",
    "CTP_02_043","CTP_02_050","CTP_02_055","CTP_02_062","CTP_03_003","CTP_03_010","CTP_03_014",
    "CTP_01_057","CTP_01_059","CTP_01_066","CTP_01_068","CTP_01_071","CTP_01_073"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "PATIENTS/SUS2020_TIFF/FINALIZE_PM_TIFF/",
    "patients": "PATIENTS/SUS2020_TIFF/FINAL_TIFF/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "mJNet_v2",
      "loss": "mod_dice_coef_loss",
      "metrics": ["mod_dice_coef"],
      "epochs": 100,
      "batch_size": 32,
      "classes_to_evaluate": ["penumbra","core","penumbracore"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 5,
        "number_patients_for_validation": 15,
        "number_patients_for_testing": 0,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "SGD",
        "learning_rate": 0.01,
        "decay": 1e-6,
        "momentum":0.9,
        "nesterov":"True"
      },
      "params":{
        "max_pool":{
          "long.1":5,
          "long.2":3,
          "long.3":2
        },
        "dropout":{
          "long.1":0,
          "1":0,
          "2":0,
          "3":0,
          "3.1":0,
          "3.2":0,
          "3.3":0.5,
          "3.4":0.5
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "val_mod_dice_coef",
          "mode": "max",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "val_loss",
          "min_delta": 0.00001,
          "patience": 12
        },
        "LearningRateScheduler": {
          "decay_step": 10,
          "decay_rate": 0.1
        },
        "CollectBatchStats": {
          "acc":"mod_dice_coef"
        }
      },
      "to_categ":0,
      "save_images": 1,
      "save_statistics": 0,
      "use_background_in_statistics": 0,
      "calculate_ROC": 0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 1,
      "save_activation_filter": 0,
      "use_hickle": 0
    }
  ]
}
