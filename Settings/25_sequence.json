{
  "EXPERIMENT": 25,
  "USE_SEQUENCE_TRAIN": 1,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/SUS2020_TIFF/",
  "PATIENTS_TO_TRAINVAL": ["ALL"],
  "PATIENTS_TO_TEST": ["01_010","01_025","01_037","01_057","01_061","01_066","02_001","02_004","02_009","02_016","02_020","02_027","03_003","03_010","03_014"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 1
  },
  "relative_paths": {
    "labeled_images": "PATIENTS/SUS2020_TIFF/FINALIZE_PM_TIFF/",
    "patients": "PATIENTS/SUS2020_TIFF/FINAL_TIFF/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "mJNet_LongJ_Drop",
      "loss": "mod_dice_coef_loss",
      "metrics": ["mod_dice_coef"],
      "epochs": 50,
      "batch_size": 16,
      "steps_per_epoch_ratio": 0.1,
      "classes_to_evaluate": ["penumbra","core","penumbracore"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 5,
        "number_patients_for_validation": 16,
        "number_patients_for_testing": 0,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{
        "max_pool":{
          "long.1":5,
          "long.2":3,
          "long.3":2
        },
        "dropout":{
          "long.1":0.3,
          "1":0,
          "2":0.3,
          "3":0.3,
          "4":0,
          "5":0
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "val_mod_dice_coef",
          "mode": "max",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "val_loss",
          "min_delta": 0.00001,
          "patience": 12
        },
        "LearningRateScheduler": {
          "decay_step": 10,
          "decay_rate": 0.01
        },
        "CollectBatchStats": {
          "acc":"mod_dice_coef"
        },
        "TensorBoard": {
          "update_freq":"batch",
          "histogram_freq": 5
        }
      },
      "to_categ":1,
      "save_images": 1,
      "save_statistics": 0,
      "use_background_in_statistics": 0,
      "calculate_ROC": 0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 1,
      "save_activation_filter": 0,
      "use_hickle": 0
    }
  ]
}
