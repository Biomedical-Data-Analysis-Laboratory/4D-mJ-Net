{
  "EXPERIMENT": 13,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/ISLES2018/Two_classes/",
  "PATIENTS_TO_TRAINVAL": ["ALL"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "NUMBER_OF_IMAGE_PER_SECTION": 64,
    "3D": 1,
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "PATIENTS/ISLES2018/NEW_TRAINING/Binary_Ground_Truth/",
    "patients": "PATIENTS/ISLES2018/NEW_TRAINING/FINAL/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "mJNet_3D",
      "loss": "weighted_categorical_cross_entropy_loss",
      "metrics": ["weighted_categorical_cross_entropy","mod_dice_coef", "dice_coef"],
      "epochs": 10,
      "batch_size":64,
      "classes_to_evaluate": ["core"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 5,
        "number_patients_for_validation": 5,
        "number_patients_for_testing": 5,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "SGD",
        "learning_rate": 0.01,
        "decay": 1e-6,
        "momentum":0.9,
        "nesterov":"True"
      },
      "params":{
        "dropout":{
          "long.1":0,
          "1":0,
          "2":0,
          "3":0,
          "3.1":0.5,
          "3.2":0.5,
          "3.3":0,
          "3.4":0,
          "4":0,
          "5":0
        },
        "max_pool":{
          "long.1":4,
          "long.2":4,
          "long.3":4
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "weighted_weighted_categorical_cross_entropy",
          "mode": "max",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "loss",
          "min_delta": 0.00001,
          "patience": 15
        },
        "LearningRateScheduler": {
          "decay_step": 5,
          "decay_rate": 0.1
        },
        "CollectBatchStats": {
          "acc":"weighted_weighted_categorical_cross_entropy"
        }
      },
      "to_categ":1,
      "save_images": 1,
      "save_statistics":1,
      "use_background_in_statistics":0,
      "calculate_ROC":0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 1,
      "save_activation_filter": 1
    }
  ]
}
