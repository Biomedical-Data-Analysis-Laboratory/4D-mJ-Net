      "train_again": 0,
      "supervised": 1
    },
    {
      "name": "mJNet_LongJ_Drop",
      "loss": "dice_coef_loss",
      "metrics": ["dice_coef", "jaccard_distance", "sensitivity", "specificity", "precision", "f1"],
      "epochs": 5,
      "val":{
        "validation_perc": 5,
        "random_validation_selection": 0
      },
