        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{
        "dropout":{
          "1":0.3,
          "2":0.3,
          "3":0.3,
          "4":0.3,
          "5":0.3
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "loss",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "loss",
