{
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/",
  "PATIENT_TO_TEST": [12,13,14,15,16],
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "2",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 1
  },
  "relative_paths": {
    "labeled_images": "Manual_annotations/",
    "patients": "PATIENTS/",
    "save": {
      "model": "SAVED_MODELS/",
      "plot": "SAVED_PLOTS/",
      "images": "SAVED_IMAGES/",
      "text": "SAVED_TEXT/"
    }
  },
  "models": {
    "mJ-Net": {
      "name": "mJNet",
      "loss": "dice_coef_loss",
      "metrics": ["dice_coef", "sensitivity", "specificity"],
      "epochs": 150,
      "validation_perc": 10,
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "loss",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "dice_coef",
          "min_delta": 0,
          "patience": 5
        },
        "ReduceLROnPlateau": {
          "monitor": "dice_coef",
          "factor": 0.0005,
          "patience": 2,
          "min_delta": 0.001,
          "min_lr": 0.00000001
        },
        "CollectBatchStats": {
          "acc":"dice_coef"
        }
      },
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 0
    }
  }
}
