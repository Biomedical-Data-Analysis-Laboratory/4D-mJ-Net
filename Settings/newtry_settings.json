{
  "EXPERIMENT": 6,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/Master2019/",
  "PATIENT_TO_USE": [12,13,14,15,16],
  "OVERRIDE_MODELS_ID_PATH": "mJNet_DA_ADAM_VAL5_02",
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "Manual_annotations/",
    "patients": "PATIENTS/Master2019/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/"
    }
  },
  "models": [
    {
      "name": "mJNet_LongJ_Drop",
      "loss": "mod_dice_coef_loss",
      "metrics": ["mod_dice_coef", "jaccard_distance", "sensitivity", "specificity", "precision", "f1"],
      "to_categ":1,
      "epochs": 10,
      "classes_to_evaluate": ["penumbra", "core", "penumbracore"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 5,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "SGD",
        "learning_rate": 0.01,
        "decay": 1e-6,
        "momentum":0.9,
        "nesterov":"True"
      },
      "params":{
        "dropout":{
          "0.1": 0.3,
          "1":0.3,
          "2":0.3,
          "3":0.3,
          "4":0.3,
          "5":0.3
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "loss",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "loss",
          "min_delta": 0.0001,
          "patience": 10
        },
        "ReduceLROnPlateau": {
          "monitor": "dice_coef",
          "factor": 0.1,
          "patience": 8,
          "min_delta": 0.001,
          "cooldown": 1,
          "min_lr": 0.00000001
        },
        "CollectBatchStats": {
          "acc":"dice_coef"
        }
      },
      "save_images": 1,
      "save_statistics":1,
      "use_background_in_statistics":0,
      "calculate_ROC":1,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 0
    }
  ]
}
