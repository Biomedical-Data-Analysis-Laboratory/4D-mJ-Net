{
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/",
  "PATIENT_TO_TEST": [12,13,14,15,16],
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "Manual_annotations/",
    "patients": "PATIENTS/",
    "save": {
      "model": "MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/"
    }
  },
  "models": [
    {
      "name": "mJNet_LongJ_Drop",
      "loss": "dice_coef_loss",
      "metrics": ["dice_coef", "sensitivity", "specificity", "precision", "f1"],
      "epochs": 100,
      "val":{
        "validation_perc": 10,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{
        "dropout":{
          "0.1": 0.3,
          "1":0.3,
          "2":0.3,
          "3":0.3,
          "4":0.3,
          "5":0.3
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "loss",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "val_loss",
          "min_delta": 0.0001,
          "patience": 10
        },
        "ReduceLROnPlateau": {
          "monitor": "dice_coef",
          "factor": 0.5,
          "patience": 2,
          "min_delta": 0.001,
          "min_lr": 0.00000001
        },
        "CollectBatchStats": {
          "acc":"dice_coef"
        }
      },
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 0
    },
    {
      "name": "mJNet_Drop",
      "loss": "dice_coef_loss",
      "metrics": ["dice_coef", "sensitivity", "specificity", "precision", "f1"],
      "epochs": 100,
      "val":{
        "validation_perc": 0,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{
        "dropout":{
          "0.1": 0.3,
          "1":0.3,
          "2":0.3,
          "3":0.3,
          "4":0.3,
          "5":0.3
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "loss",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "loss",
          "min_delta": 0.0001,
          "patience": 10
        },
        "ReduceLROnPlateau": {
          "monitor": "dice_coef",
          "factor": 0.5,
          "patience": 2,
          "min_delta": 0.001,
          "cooldown": 5,
          "min_lr": 0.00000001
        },
        "CollectBatchStats": {
          "acc":"dice_coef"
        }
      },
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 0
    }
  ]
}
