{
  "EXPERIMENT": 14,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/SUS2020_v2/",
  "PATIENT_TO_TEST": ["ALL"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "3D": 1,
    "ONE_TIME_POINT": 17,
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "PATIENTS/SUS2020_v2/FINALIZE_PM/",
    "patients": "PATIENTS/SUS2020_v2/FINAL/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "simple_autoencoder",
      "loss": "mean_squared_error",
      "metrics": ["weighted_categorical_cross_entropy","mod_dice_coef","dice_coef"],
      "epochs": 200,
      "batch_size":2,
      "classes_to_evaluate": ["core"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 5,
        "number_patients_for_validation": 1,
        "number_patients_for_testing": 1,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{},
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "weighted_mod_dice_coef",
          "mode": "max",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "loss",
          "min_delta": 0.00001,
          "patience": 15
        },
        "LearningRateScheduler": {
          "decay_step": 5,
          "decay_rate": 0.1
        },
        "CollectBatchStats": {
          "acc":"weighted_mod_dice_coef"
        }
      },
      "to_categ":1,
      "save_images": 1,
      "save_statistics":1,
      "use_background_in_statistics":0,
      "calculate_ROC":0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 0,
      "save_activation_filter": 1
    }
  ]
}
