{
  "EXPERIMENT": 21,
  "root_path": "/home/stud/lucat/PhD_Project/Stroke_segmentation/",
  "dataset_path": "/local/home/lucat/DATASET/SUS2020_tmp/",
  "PATIENTS_TO_TRAINVAL": ["99_017","99_018","99_073","99_006","99_007","99_009","99_002","99_071","99_068","99_066","99_059","99_057"],
  "OVERRIDE_MODELS_ID_PATH": "",
  "init": {
    "TF_CPP_MIN_LOG_LEVEL": "3",
    "per_process_gpu_memory_fraction": 1,
    "allow_growth": 1,
    "MULTIPROCESSING": 0
  },
  "relative_paths": {
    "labeled_images": "PATIENTS/SUS2020_tmp/FINALIZE_PM/",
    "patients": "PATIENTS/SUS2020_tmp/FINAL/",
    "save": {
      "model": "MODELS/",
      "partial_model": "TMP_MODELS/",
      "plot": "PLOTS/",
      "images": "IMAGES/",
      "text": "TEXT/",
      "intermediate_activation": "intermediate_activation/"
    }
  },
  "models": [
     {
      "name": "mJNet_LongJ_Drop",
      "loss": "tversky_loss",
      "metrics": ["mod_dice_coef","tversky"],
      "epochs": 150,
      "batch_size": 1024,
      "classes_to_evaluate": ["penumbra","core","penumbracore"],
      "statistics": ["sensitivity", "specificity", "precision", "f1", "accuracy", "jaccard_index"],
      "val":{
        "validation_perc": 10,
        "number_patients_for_validation": 0,
        "number_patients_for_testing": 2,
        "random_validation_selection": 0
      },
      "test_steps":1,
      "optimizer": {
        "name": "Adam",
        "lr": 0.001,
        "beta_1": 0.9,
        "beta_2":0.999,
        "epsilon":"None",
        "decay":0.0
      },
      "params":{
        "max_pool":{
          "long.1":5,
          "long.2":3,
          "long.3":2
        },
        "dropout":{
          "long.1":0.3,
          "1":0.3,
          "2":0.3,
          "3":0.3,
          "3.1":0.3,
          "3.2":0.3,
          "3.3":0.3,
          "3.4":0.3,
          "4":0.3,
          "5":0.3
        }
      },
      "callbacks":{
        "ModelCheckpoint": {
          "monitor": "val_tversky_loss",
          "mode": "max",
          "period": 1
        },
        "EarlyStopping": {
          "monitor": "val_loss",
          "min_delta": 0.00001,
          "patience": 20
        },
        "LearningRateScheduler": {
          "decay_step": 20,
          "decay_rate": 0.1
        },
        "CollectBatchStats": {
          "acc":"tversky"
        }
      },
      "to_categ":0,
      "save_images": 1,
      "save_statistics": 0,
      "use_background_in_statistics": 0,
      "calculate_ROC": 0,
      "data_augmentation": 1,
      "cross_validation": 0,
      "train_again": 0,
      "supervised": 1,
      "save_activation_filter": 0,
      "use_hickle": 0
    }
  ]
}
